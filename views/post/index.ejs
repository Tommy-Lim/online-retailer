<h1>all posts:</h1>
<div class="row">
  <div class="col-sm-3">
    filters
  </div>
  <div class="col-sm-9">
    <% products.forEach(function(product){ %>
      <div>
        <div class="row">
          <div class="col-xs-10">
            <%- product.title %>
          </div>
          <div class="col-xs-2">
            <%- product.price %>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12 col-xs-offset-1">
            <%- product.location %>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12 col-xs-offset-1">
            <%- product.details %>
          </div>
        </div>
        <div class="row">
          <div class="col-xs-12 col-xs-offset-1">
            buttons
          </div>
        </div>
      </div>
    <% }); %>
  </div>
</div>
